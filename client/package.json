{
  "name": "moodly-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bigpopakap.github.io/moodly/",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-scripts": "2.1.5",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "scripts": {
    "lint": "npx eslint --ext ts,tsx ./ && npx stylelint **/*.{css,module.css,scss,module.scss}",
    "lint:fix": "npx eslint --ext ts,tsx --fix ./ && npx stylelint **/*.{css,module.css,scss,module.scss} --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "hard-reset": "rm package-lock.json && rm -rf ./node_modules"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
